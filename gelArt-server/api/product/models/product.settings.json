{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "discount": {
      "type": "integer"
    },
    "main_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tags": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "unitPresentationKg": {
      "type": "enumeration",
      "enum": [
        "Tarro",
        "Funda"
      ]
    },
    "presentationKg": {
      "type": "integer"
    },
    "typeProduct": {
      "type": "enumeration",
      "enum": [
        "Pasteleria",
        "Heladeria"
      ]
    },
    "Identifier": {
      "type": "string"
    },
    "productHeladeria": {
      "type": "json"
    }
  }
}
